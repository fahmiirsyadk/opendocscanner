{
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "routes": [
    { "src": "/src/Worker/processor\\.worker\\.js", "dest": "/src/Worker/processor.worker.js" },
    { "src": "/bundle\\.min\\.js", "dest": "/bundle.min.js" },
    { "src": "/bundle\\.js", "dest": "/bundle.js" },
    { "src": "/(.*\\.(js|mjs|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))", "dest": "/$1" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/bundle.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/bundle.min.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}
