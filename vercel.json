{
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "routes": [
    { "src": "/src/Worker/processor\\.worker\\.js", "dest": "/src/Worker/processor.worker.js", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/bundle\\.min\\.js", "dest": "/bundle.min.js", "headers": { "Cache-Control": "public, max-age=86400" } },
    { "src": "/bundle\\.js", "dest": "/bundle.js", "headers": { "Cache-Control": "public, max-age=86400" } },
    { "src": "/(.*\\.(js|mjs|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))", "dest": "/$1", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/index\\.html", "dest": "/index.html", "headers": { "Cache-Control": "no-cache, no-store, must-revalidate" } },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
